<%= form_with model: category, class: "" do |form| %>

    <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>

    <div>
        <%= form.label :details %>
        <%= form.text_area :details %>
    </div>

    <div>
        <%= form.submit %>
    </div>

<% end %>